        "headers": {
          "Auth-Token": "string"
        },
        "body": {
          "employeeData": "object"
        }
      },
      "response": {
        "success": true
      }
    },
    "saveSafeAccount": {
      "request": {
        "method": "POST",
        "endpoint": "/api/safe-account",
        "headers": {
          "Auth-Token": "string"
        },
