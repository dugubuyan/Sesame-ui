      "request": {
        "method": "POST",
        "endpoint": "/api/login",
        "body": {
          "walletAddress": "string"
        }
      },
      "response": {
        "success": true,
        "data": {
          "authToken": "string"
        }
      }
    },
    "dashboard": {
      "request": {
        "method": "GET",
        "endpoint": "/api/dashboard",
        "headers": {
          "Auth-Token": "string"
        }
      },
      "response": {
        "success": true,
        "data": {
          "totalEmployees": "number",
          "totalPayroll": "number",
          "pendingPayments": "number",
          "recentTransactions": "array"
        }
      }
    },
    "payroll": {
      "request": {
        "method": "GET",
        "endpoint": "/api/payroll",
